create Stage{
    script[]
    costumes[
        "blank.svg"
    ]
    sounds[]
}

create cat{
    script[
        event_whenflagclicked();
        motion_movesteps(5);
        event_whenkeypressed("space");
        motion_gotoxy(3, 4);
    ]
    costumes[
        "cat.svg"
    ]
}

create man(
    script[
        event_whenflagclicked(); #when the flag is clicked,
        motion_movesteps(10); # move 10 steps
    ]
    costumes[
        "cat.svg",
        "blank.svg"
    ]
)